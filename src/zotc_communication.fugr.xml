      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_INTAD</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Internet address of partner company clerk</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_ADRNR_SENDER</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Address</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_STATUS</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Document Status</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_PRINT_DOC</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Print Flag</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>NO_EMAIL_ADDRESS</PARAMETER>
       <KIND>X</KIND>
       <STEXT>E-mail address not maintained</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>NO_FAX_NUMBER</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Fax number not maintained</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>MAIL_FAILURE</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Mail sending failed</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>FAX_FAILURE</PARAMETER>
       <KIND>X</KIND>
       <STEXT>Fax sending failed</STEXT>
      </RSFDO>
     </DOCUMENTATION>
