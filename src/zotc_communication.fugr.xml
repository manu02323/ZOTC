   <FUNCTIONS>
    <item>
     <FUNCNAME>ZOTC_STANDARD_COMMUNICATION</FUNCNAME>
     <SHORT_TEXT>External Communication for Mail/Fax/Print</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>IM_ADRNR</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>ADRNR</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_SUBJECT</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>SO_OBJ_DES</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_FORM_OUTPUT</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>FPFORMOUTPUT</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_T_TEXT</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>BCSY_TEXT</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_ATTACHMENT_NAME</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>SOOD-OBJDES</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_TLFXS</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>TLFXS</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_INTAD</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>INTAD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_ADRNR_SENDER</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>ADRNR</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EX_STATUS</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>STRING</TYP>
      </RSEXP>
      <RSEXP>
       <PARAMETER>EX_PRINT_DOC</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>BOOLEAN</TYP>
      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>NO_EMAIL_ADDRESS</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>NO_FAX_NUMBER</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>MAIL_FAILURE</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>FAX_FAILURE</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IM_ADRNR</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Address No.</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_SUBJECT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Mail Subject</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_FORM_OUTPUT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Form Output (PDF, PDL)</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_T_TEXT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Text Content</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_ATTACHMENT_NAME</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Short description of contents</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_TLFXS</PARAMETER>
       <KIND>P</KIND>
