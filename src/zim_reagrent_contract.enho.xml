    <PGMID>R3TR</PGMID>
    <ORG_OBJ_TYPE>CLAS</ORG_OBJ_TYPE>
    <ORG_OBJ_NAME>CL_SLS_SALESORDERERPCRTRC2</ORG_OBJ_NAME>
    <ORG_MAIN_TYPE>CLAS</ORG_MAIN_TYPE>
    <ORG_MAIN_NAME>CL_SLS_SALESORDERERPCRTRC2</ORG_MAIN_NAME>
